                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	URNId,
	MAX(Code) AS Code,
	CASE WHEN MAX(UseIncomeTax) = 1 THEN SUM(ReceiptQuantity * PricePerDealUnit) * 0.1 ELSE 0 END AS PPN,
	SUM(ReceiptQuantity * PricePerDealUnit) AS DPP,
	'' AS Products,
	MAX(SupplierCode) AS SupplierCode,
	MAX(SupplierName) AS SupplierName,
	MAX(SupplierId) AS SupplierId,
	CAST(MAX(SupplierIsImport) AS BIT) AS SupplierIsImport,
	MAX(Date) AS Date,
	MAX(Currency) AS Currency,
	MAX(PaymentDuration) AS PaymentDuration,
	MAX(DivisionCode) AS DivisionCode,
	MAX(DivisionId) AS DivisionId,
	MAX(DivisionName) AS DivisionName,
	MAX(UnitCode) AS UnitCode,
	MAX(UnitId) AS UnitId,
	MAX(UnitName) AS UnitName,
	MAX(CreatedAgent) AS CreatedAgent,
	MAX(CreatedBy) AS CreatedBy,
	MAX(CreatedUtc) AS CreatedUtc,
	MAX(CreatedAgent) AS LastModifiedAgent,
	MAX(CreatedBy) AS LastModifiedBy,
	MAX(CreatedUtc) AS LastModifiedUtc,
	'' AS DeletedAgent,
	'' AS DeletedBy,
	'0001-01-01' AS DeletedUtc
FROM (SELECT 
	URNId,
	URNItemId,
	MAX(ReceiptQuantity) AS ReceiptQuantity,
	MAX(PricePerDealUnit) AS PricePerDealUnit,
	MAX(ProductName) AS ProductName,
	MAX(SupplierCode) AS SupplierCode,
	MAX(SupplierId) AS SupplierId,
	MAX(SupplierIsImport + 0) AS SupplierIsImport,
	MAX(SupplierName) AS SupplierName,
	MAX(URNNo) AS Code,
	MAX(ReceiptDate) AS Date,
	MAX(DivisionCode) AS DivisionCode,
	MAX(DivisionId) AS DivisionId,
	MAX(DivisionName) AS DivisionName,
	MAX(UnitCode) AS UnitCode,
	MAX(UnitId) AS UnitId,
	MAX(UnitName) AS UnitName,
	MAX(UseIncomeTax + 0) AS UseIncomeTax,
	MAX(CurrencyCode) AS Currency,
	MAX(PaymentDuration) AS PaymentDuration,
	MAX(CreatedAgent) AS CreatedAgent,
	MAX(CreatedBy) AS CreatedBy,
	MAX(CreatedUtc) AS CreatedUtc
FROM (SELECT * FROM (SELECT 
	URN.Id AS URNId,
	URN.CreatedUtc,
	URN.CreatedBy,
	URN.CreatedAgent,
	URN.IsDeleted,
	URNItem.Id AS URNItemId,
	URNItem.ReceiptQuantity,
	URNItem.PricePerDealUnit,
	URNItem.ProductName,
	URN.SupplierCode,
	URN.SupplierId,
	URN.SupplierIsImport,
	URN.SupplierName,
	URN.ReceiptDate,
	URN.URNNo,
	CAST(URN.DivisionId AS INT) AS DivisionId,
	URN.DivisionCode,
	URN.DivisionName,
	URN.UnitCode,
	CAST(URN.UnitId AS INT) AS UnitId,
	URN.UnitName,
	EPODetail.Id AS EPODetailId,
	EPO.UseIncomeTax AS UseIncomeTax,
	EPO.CurrencyCode AS CurrencyCode,
	EPO.PaymentDueDays AS PaymentDuration
FROM UnitReceiptNotes  URN

LEFT JOIN UnitReceiptNoteItems URNItem
ON URN.Id = URNItem.URNId

LEFT JOIN ExternalPurchaseOrderDetails EPODetail
ON URNItem.EPODetailId = EPODetail.Id

LEFT JOIN ExternalPurchaseOrderItems EPOItem
ON EPODetail.EPOItemId = EPOItem.Id

LEFT JOIN ExternalPurchaseOrders EPO
ON EPOItem.EPOId = EPO.Id

WHERE URN.IsDeleted = 0) A
WHERE EPODetailId IS NOT NULL) B
GROUP BY URNId, URNItemId) C
GROUP BY URNId</property>
